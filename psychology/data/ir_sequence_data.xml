<data noupdate="1">
<!-- Psychology Case -->
<record id="sequence_psychology_case" model="ir.sequence">
    <field name="name">Psychology Case</field>
    <field name="code">psychology.case</field>
    <field name="padding">6</field>
    <field name="prefix">CASE/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>

<!-- Psychology Service -->
<record id="sequence_psychology_service" model="ir.sequence">
    <field name="name">Psychology Service</field>
    <field name="code">psychology.service</field>
    <field name="padding">6</field>
    <field name="prefix">SRV/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>

<!-- Psychology Activity -->
<record id="sequence_psychology_activity" model="ir.sequence">
    <field name="name">Psychology Activity</field>
    <field name="code">psychology.activity</field>
    <field name="padding">6</field>
    <field name="prefix">ACT/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>
</data>
